package org.kh.java;

public class Practice2 {

	public static void main(String[] args) {
		int a = 10;
		float b = 2.0f;
		
		System.out.println("a+b = "+(int)(a+b));
		System.out.println("a-b = "+(int)(a-b));
		System.out.println("a*b = "+(int)(a*b));
		System.out.println("a/b = "+(int)(a/b));

	}

}
